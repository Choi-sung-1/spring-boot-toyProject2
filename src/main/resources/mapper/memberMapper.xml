<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.toyProject2.mapper.MemberMapper">
    <insert id="insert" parameterType="com.project.toyProject2.domain.vo.MemberVO" keyProperty="memberId" useGeneratedKeys="true">
        INSERT INTO TBL_MEMBER(MEMBER_LOGIN_ID, MEMBER_PASSWORD, MEMBER_NAME, MEMBER_ROLE, MEMBER_REGISTER_DATE, MEMBER_UPDATE_DATE)
        VALUES (#{memberLoginId},#{memberPassword},#{memberName},#{memberRole},#{memberRegisterDate},#{memberUpdateDate})
    </insert>
    <select id="selectMemberById">
        SELECT * FROM TBL_MEMBER WHERE MEMBER_LOGIN_ID=#{memberLoginId}
    </select><select id="selectMemberByPk">
        SELECT * FROM TBL_MEMBER WHERE MEMBER_ID=#{memberId}
    </select>
</mapper>